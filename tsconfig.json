{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictBindCallApply": false,
    "outDir": "./dist"
  },
  "include": ["src"]
}
